{
  "private": true,
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "storybook": "start-storybook -p 9001",
    "clean": "rm -rf public/*.js public/*.map public/*.css public/*.gz",
    "compile": "node -r dotenv/config -r babel-register --harmony bin/compile",
    "dev": "concurrently --kill-others 'yarn run watch' 'php artisan serve'",
    "dev:test": "concurrently --kill-others 'yarn run watch' 'php artisan serve' 'yarn run test:dev'",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "watch": "yarn run clean && nodemon -x 'node -r dotenv/config -r babel-register --harmony' bin/watch.js",
    "lint": "eslint . ./",
    "lint:fix": "yarn run lint -- --fix",
    "test": "better-npm-run test",
    "test:dev": "yarn run test -- --watch"
  },
  "betterScripts": {
    "deploy": {
      "command": "yarn run clean && yarn run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy:dev": {
      "command": "yarn run clean && yarn run compile",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/tests/mocks/style-mock.js",
      "flexboxgrid": "<rootDir>/tests/mocks/style-mock.js"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "./src/bootstrap.js"
    ],
    "setupTestFrameworkScriptFile": "./tests/jest-env.js"
  },
  "greenkeeper": {
    "ignore": [
      "history"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunday-Without-God/Juice.git"
  },
  "author": "DanSnow",
  "dependencies": {
    "any-promise": "^1.3.0",
    "axios": "^0.15.3",
    "babel-polyfill": "^6.23.0",
    "basscss-hide": "^1.0.1",
    "better-npm-run": "^0.0.14",
    "classnames": "^2.2.5",
    "codemirror": "^5.24.2",
    "date-fns": "^1.28.0",
    "debug": "^2.6.1",
    "decko": "^1.2.0",
    "dotenv": "^4.0.0",
    "fbjs": "^0.8.9",
    "flexboxgrid": "^6.3.1",
    "github-markdown-css": "^2.4.1",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "invariant": "^2.2.2",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.0",
    "normalizr": "^3.2.2",
    "p-try": "^1.0.0",
    "path-to-regexp": "^1.7.0",
    "pinkie": "^2.0.4",
    "prism-themes": "^1.0.0",
    "prismjs": "^1.6.0",
    "radium": "^0.18.1",
    "react": "^15.4.2",
    "react-clipboard.js": "^1.0.1",
