{
  "private": true,
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "storybook": "start-storybook -p 9001",
    "clean": "rm -rf public/*.js public/*.map public/*.css",
    "compile": "node -r dotenv/config -r babel-register --harmony bin/compile",
    "dev": "concurrently --kill-others 'npm run watch' 'php artisan serve'",
    "dev:test": "concurrently --kill-others 'npm run watch' 'php artisan serve' 'npm run test:dev'",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "watch": "npm run clean && nodemon -x 'node -r dotenv/config -r babel-register --harmony' bin/watch.js",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch"
  },
  "betterScripts": {
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy:dev": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/tests/mocks/style-mock.js",
      "flexboxgrid": "<rootDir>/tests/mocks/style-mock.js"
    },
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "./src/bootstrap.js"
    ],
    "setupTestFrameworkScriptFile": "tests/jest-env.js",
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "react-test-renderer",
      "enzyme",
      "recompose",
      "immutable",
      "redux-mock-store",
      "redux-actions",
      "redux-thunk",
      "lodash",
      "bluebird",
      "core-js",
      "material-ui",
      "react-flexbox-grid"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunday-Without-God/Juice.git"
  },
  "author": "DanSnow",
  "dependencies": {
    "axios": "^0.13.1",
    "babel-polyfill": "^6.13.0",
    "better-npm-run": "0.0.11",
    "bluebird": "^3.4.1",
    "classnames": "^2.2.5",
    "codemirror": "^5.18.0",
    "debug": "^2.2.0",
    "decko": "^1.1.3",
    "dotenv": "^2.0.0",
    "fbjs": "^0.8.4",
    "flexboxgrid": "^6.3.1",
    "github-markdown-css": "^2.4.0",
    "highlight.js": "^9.6.0",
    "history": "^2.1.2",
    "immutable": "^3.8.1",
    "invariant": "^2.2.1",
    "lodash": "^4.15.0",
    "material-ui": "^0.15.4",
    "normalizr": "^2.2.1",
    "path-to-regexp": "^1.5.3",
    "qwery": "^4.0.0",
    "radium": "^0.18.1",
    "react": "^15.3.1",
    "react-clipboard.js": "^0.2.5",
    "react-codemirror": "^0.2.6",
    "react-dom": "^15.3.1",
    "react-flexbox-grid": "^0.10.2",
    "react-highlight": "^0.8.0",
    "react-motion": "^0.4.4",
    "react-recaptcha": "^2.1.1",
    "react-redux": "^4.4.5",
    "react-remarkable": "^1.1.1",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-router-scroll": "^0.3.2",
    "react-tap-event-plugin": "^1.0.0",
    "recompose": "^0.20.2",
    "redux": "^3.5.2",
    "redux-actions": "^0.11.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.3",
    "ress": "^1.1.1",
    "store": "^1.3.20",
    "validate.js": "^0.10.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.3.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "cheerio": "^0.20.0",
