    "jest-cli": "^14.1.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.10.1",
    "react-addons-test-utils": "^15.3.1",
    "react-test-renderer": "^15.3.1",
    "redux-mock-store": "^1.1.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
