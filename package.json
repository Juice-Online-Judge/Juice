{
  "private": true,
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "storybook": "start-storybook -p 9001",
    "clean": "rm -rf public/*.js public/*.map public/*.css",
    "compile": "node -r dotenv/config --harmony bin/compile",
    "deploy": "better-npm-run deploy",
    "watch": "nodemon -x 'node -r dotenv/config --harmony' bin/watch.js",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch"
  },
  "betterScripts": {
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "node -r dotenv/config ./node_modules/karma/bin/karma start bin/karma.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunday-Without-God/Juice.git"
  },
  "author": "DanSnow",
